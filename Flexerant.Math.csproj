<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.11</Version>
    <PackageProjectUrl>https://github.com/flexerant/Math</PackageProjectUrl>
    <RepositoryUrl>https://github.com/flexerant/Math</RepositoryUrl>
    <Description>Flexerant.Math is a .NET utility containing the following algorithms; Net Present Value (NPV), Binary search, Bisection method, Netwon-Rapshon method, Brent's method, First derivative.
</Description>
    <PackageLicenseExpression>https://github.com/flexerant/Math/blob/main/LICENSE</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Optimization.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove=".gitignore" />
  </ItemGroup>

</Project>
